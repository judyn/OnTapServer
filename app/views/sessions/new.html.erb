
  <h2>Creds</h2>
  <%= form_tag sessions_path do %>
  <div class="form-group">
    <%= label_tag :email, "Email Address"%>
    <a href="https://rubygems.org/gems/catpants">Cat Pants Gem! Always read documentation :)</a>
    <%= text_field_tag :display_name, "", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= label_tag :password, "Password",id:"catpants" %>
    <%= password_field_tag :password, "", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= submit_tag "Sign In", class: "btn btn-primary" %>
  </div>
  <% end %>
