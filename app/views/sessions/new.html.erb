
  <h2>Creds</h2>
  <%= form_tag sessions_path do %>
  <div class="form-group">
    <%= label_tag :email, "Email Address"%>
    <%= text_field_tag :email, "", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= label_tag :password, "Password",class:"catpants" %>
    <%= password_field_tag :password, "", class: "form-control catpants-password" %>
    <a href="https://rubygems.org/gems/catpants">Cat Pants Gem!</a>
    <br/>
  </div>
  <div class="form-group">
    <%= submit_tag "Sign In", class: "btn btn-primary" %>
  </div>
  <% end %>
